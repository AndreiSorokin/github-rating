{
   "plugins": ["react", "react-native"],
   "settings": {
     "react": {
       "version": "detect"
     }
   },
   "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:jest/recommended", "plugin:react-native/all"],
   "parser": "@babel/eslint-parser",
   "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
   "env": {
      "react-native/react-native": true,
      "browser": true,
      "node": true,
      "es6": true
   },
   "rules": {
     "react/prop-types": "off",
     "react/react-in-jsx-scope": "off"
   }
}